  "authors": [
    "wilechaote",
    "XGJS"
  ],
  "contact": {
    "homepage": "https://github.com/XGJSJS/rsgauges",
    "sources": "https://github.com/XGJSJS/rsgauges/issues/"
  },
  "license": "MIT",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
